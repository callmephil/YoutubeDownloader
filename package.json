{
  "name": "ytmusicmanager",
  "version": "1.0.0",
  "description": "Music Manager Based Youtube API Educational Project",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
